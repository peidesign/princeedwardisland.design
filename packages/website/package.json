{
  "name": "@peidesign/website",
  "private": true,
  "version": "2.0.0",
  "description": "The website and/or progressive web app (PWA) for Prince Edward Island Design",
  "collection": "dist/components/collection/collection-manifest.json",
  "types": "dist/components/types/components.d.ts",
  "main": "dist/components/index.js",
  "module": "dist/components/index.mjs",
  "es2015": "dist/esm/index.mjs.js",
  "es2017": "dist/esm/index.mjs.js",
  "jsnext:main": "dist/esm/index.mjs.js",
  "scripts": {
    "start": "npx netlify dev",
    "develop": "npm run build.11ty && npx run-p develop.*",
    "develop.11ty": "npm run build.11ty -- --watch --output=dist/www/content",
    "develop.stencil": "npx stencil build --dev --watch --serve --port=3333",
    "build": "npx run-s build.*",
    "build.11ty": "npx eleventy --config=eleventy.config.js",
    "build.stencil": "npx stencil build --prerender",
    "test": "npx stencil test --spec --e2e",
    "test.watch": "npx stencil test --spec --e2e --watchAll"
  },
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.8.3",
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "@peidesign/design-system": "^0.1.0",
    "@stencil/core": "^1.0.0-alpha.23",
    "@stencil/postcss": "^0.1.0",
    "@stencil/router": "^0.3.4-2",
    "@stencil/state-tunnel": "0.0.9-5",
    "cssnano": "^4.1.10",
    "fast-glob": "^2.2.6",
    "postcss": "^7.0.16",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.6.0",
    "stencil-plugin-postcss-extra": "^0.1.0"
  }
}
